#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int ab,ac,x1,x2,x3,y1,y2,y3;
    cin>>x1>>y1>>x2>>y2>>x3>>y3;
    ac=sqrt((x3-x1)*(x3-x1)+(y3-y1)*(y3-y1));
    ab=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    if (ac>ab)
    cout<<"B"<<ab;
    else cout<<"C"<<ac;
    return 0;
}
