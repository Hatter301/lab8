#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if ((n>0)&&((n%2)==0))
    cout<<"Положительное чётное число";
    else if ((n>0)&&((n%2)!=0))
    cout<<"Положительное нечётное число";
    else if ((n<0)&&((n%2)!=0))
    cout<<"Отрицательное нечётное число";
    else if ((n<0)&&((n%2)==0))
    cout<<"Отрицательное чётное число";
    else cout<<"Нулевое число";
    return 0;
}
