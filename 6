#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if (((n%2)==0)&&(n>9)&&(n<100))
    cout<<"Двузначное чётное число";
    else if (((n%2)!=0)&&(n>9)&&(n<100))
    cout<<"Двузначное нечётное число";
    else if (((n%2)==0)&&(n>99)&&(n<1000))
    cout<<"Трёхзначное чётное число";
    else if (((n%2)!=0)&&(n>99)&&(n<1000))
    cout<<"Трёхзначное нечётное число";
    else if (((n%2)==0)&&(n>0)&&(n<10))
    cout<<"Однозначное чётное число";
    else if (((n%2)!=0)&&(n>0)&&(n<10))
    cout<<"Однозначное нечётное число";
    else cout<<"Данные введены некорректно";
    return 0;
}
